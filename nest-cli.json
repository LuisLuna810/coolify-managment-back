{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": false,
    "watchMode": true,
    "assets": [
      {
        "include": "**/*.json",
        "outDir": "dist",
        "watchAssets": true
      }
    ]
  },
  "projects": {},
  "monorepo": false,
  "root": "."
}
